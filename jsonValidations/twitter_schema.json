{
  "id": "http://json-schema.org/geo",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Tweets",
  "description": "Airline tweet sentiments",
  "type": "object",
  "properties": {
    "airline": {
      "description": "The name of the airline",
      "type": "string"
    },
    "airline_sentiment": {
      "description": "How the tweet feels about the airline",
      "type": "string"
    },
    "name": {
      "description": "Name of the person who made the tweet",
      "type": "string"
    },
    "text": {
      "description": "Contents of the tweet",
      "type": "string"
    },
    "negative_reason": {
      "description": "Why the rating is bad",
      "type": "string"
    }
  },
  "required": ["airline", "airline_sentiment", "name", "text"]
}
