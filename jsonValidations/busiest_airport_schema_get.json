{
    "id": "http://json-schema.org/geo",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Busiest airports",
    "description": "Data about the busiest airports when getting",
    "type": "object",
    "properties": {
      "rank": {
        "description": "The rank of the airport",
        "type": "number",
        "minimum": 0
      },
      "year": {
        "description": "The year in which the data is reported",
        "type": "number",
        "minimum": 0,
        "maximum": 2021
      }
    },
    "required": [
      "rank",
      "year"
    ]
  }