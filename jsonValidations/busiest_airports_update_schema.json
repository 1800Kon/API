{
  "id": "http://json-schema.org/geo",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Busiest airports update schema",
  "description": "Data about the busiest airports when updating",
  "type": "object",
  "properties": {
    "ID": {
        "description": "Unique ID",
        "type": "number",
        "minimum": 0
    },
    "rank": {
      "description": "The rank of the airport",
      "type": "number",
      "minimum": 0
    },
    "airport": {
      "description": "The airport name",
      "type": "string"
    },
    "total_passengers": {
      "description": "The number of passengers which arrived in the specified airport",
      "type": "number"
    }
  },
  "required": ["rank", "airport", "total_passengers"]
}
